{
	"name": "Social Ad Viewer",
	"version": "1.0.0",
	"description": "",
	"manifest_version": 3,
	"incognito": "split",
	"icons": {
		"128": "icons/128.png"
	},
	"action": {
        "default_icon": "icons/128.png",
        "default_popup": "popup/popup.html"
    },
	"background": {
		"service_worker": "background.js"
	},
	"permissions": [
        "storage",
        "scripting",
        "tabs"
	],
    "host_permissions": ["https://www.facebook.com/*"]
}